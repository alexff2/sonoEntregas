--UPDATE PRODUTOS SET CODCATEGORIA = 19 WHERE CODCATEGORIA = 110 AND ATIVO = 'S'

SELECT * 
FROM PRODUTOS WHERE CODCATEGORIA = 110 AND CODIGO IN (
SELECT CODPRODUTO FROM NVENDI2 A
INNER JOIN NVENDA2 B ON A.NUMVENDA = B.CODIGOVENDA
WHERE B.EMISSAO BETWEEN '2025-06-01' AND '2025-07-30')

